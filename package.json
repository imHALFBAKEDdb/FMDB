  "jest": {
    "transform": {
      "^.+\\.jsx$": "babel-jest",
      "^.+\\.js$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "!**/lcov-report/**",
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/index.jsx",
      "!**/webpack.config.js",
      "!**/static/**",
      "!**/server/**",
      "!**/database/**"
    ],
    "coverageThreshold": {
      "./client/components": {
        "statements": 65
      }
    }
  },
  "repository": {
    "type": "git",
